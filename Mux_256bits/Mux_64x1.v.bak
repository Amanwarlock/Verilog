


module Mux_64x1 (
			input [63:0] a_in,
			input [5:0] sel,
			output out
		);

	wire [1:0] w;
	
	Mux_32x1 m1(.sel(sel[4:0]), .a_in(a_in[31:0]), .out(w[0]));
	Mux_32x1 m2(.sel(sel[4:0]), .a_in(a_in[63:32]), .out(w[1]));
	Mux_32x1 m3(.sel({4'b0, sel[5]}), .a_in({30'b0, w}), .out(out));
	

endmodule